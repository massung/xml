<?xml version="1.0" encoding="utf-8"?><!-- processing instructions before the DOCTYPE --><?xml-stylesheet href="test.css"?><!-- make sure the DOCTYPE parses --><!DOCTYPE test PUBLIC "public-id" "system-uri" [<!-- ignored, but make sure we can parse DTD elements --><!ELEMENT test (#PCDATA)><!ELEMENT p (#PCDATA|a|ul|b|i|em)*><!-- sample notation --><!NOTATION png PUBLIC "png file" "png_viewer.exe"><!-- sample attribute lists --><!ATTLIST pre xml:space (preserve|not) #FIXED 'preserve'><!ATTLIST termdef id    ID    #REQUIRED                  name  CDATA #IMPLIED><!-- entities are used, create a pathalogical case --><!ENTITY % xx '&#37;zz;'><!ENTITY % zz '&#60;!ENTITY tricky "error-prone">'><!-- should create the ENTITY tricky -->%xx;<!-- an entity that is read from a source file --><!ENTITY % source-file SYSTEM "./test_include.xml"><!-- should create the included_entity -->%source-file;<!-- a processing instruction inside the DTD --><?dtd-processing-instruction should be in the xml-doc...?>]><!-- root tag --><test>  <?tag-processing-instruction should be in the xml-tag-node?>  <!-- add a child tag -->  <child><![CDATA[This is CDATA in the <child> tag.]]></child>  <!-- create an empty tag, put some text around it -->  &#x2022; Text before the &lt;empty&gt; tag.  <empty name='&included_entity;' />  &#x2022; Text after the &lt;empty&gt; tag.</test>